--
-- Table structure for table `domain_modification`
--

DROP TABLE IF EXISTS `domain_modification`;
SET @saved_cs_client     = @@character_set_client;
SET character_set_client = utf8;
CREATE TABLE `domain_modification` (
  `dm_id` int(255) NOT NULL auto_increment,
  `dm_doid` int(255) NOT NULL,
  `dm_action` enum('adding','removing','updating','incrementing') default NULL,
  `dm_when` datetime NOT NULL,
  `dm_by` int(255) NOT NULL,
  `dm_data` longtext,
  `dm_dostatus` enum('disabled','suspended','enabled','migrating out','migrating in','registering','desired','disputed') default NULL,
  PRIMARY KEY  (`dm_id`)
) ENGINE=MyISAM DEFAULT CHARSET=latin1 COMMENT='these are pending changes, could be a state change ';
SET character_set_client = @saved_cs_client;

--
